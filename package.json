{
  "name": "shardis",
  "version": "0.0.8",
  "description": "shardis project",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/shardis/shardis.git"
  },
  "author": "Shardis",
  "license": "Apache-2",
  "bugs": {
    "url": "https://github.com/shardis/shardis/issues"
  },
  "private": true,
  "dependencies": {
    "@angular/common": "2.4.10",
    "@angular/compiler": "2.4.10",
    "@angular/core": "2.4.10",
    "@angular/forms": "2.4.10",
    "@angular/http": "2.4.10",
    "@angular/platform-browser": "2.4.10",
    "@angular/platform-browser-dynamic": "2.4.10",
    "@angular/router": "3.4.10",
    "@ngrx/core": "1.2.0",
    "@ngrx/effects": "2.0.2",
    "@ngrx/router-store": "1.2.5",
    "@ngrx/store": "2.2.1",
    "@ngrx/store-devtools": "3.2.4",
    "@webcomponents/custom-elements": "1.0.0-alpha.3",
    "clarity-angular": "0.8.12",
    "clarity-icons": "0.8.12",
    "clarity-ui": "0.8.12",
    "core-js": "2.4.1",
    "mutationobserver-shim": "0.3.2",
    "ng2-webstorage": "1.5.1",
    "ngrx-store-freeze": "0.1.9",
    "ngrx-store-localstorage": "0.1.5",
    "reselect": "2.5.4",
    "rxjs": "5.2.0",
    "ts-helpers": "1.1.2",
    "web-animations-js": "2.2.2",
    "zone.js": "0.7.2"
  },
  "devDependencies": {
    "@angular/cli": "1.0.0",
    "@angular/compiler-cli": "2.4.10",
    "@types/jasmine": "2.5.46",
    "@types/node": "7.0.10",
    "angular2-dependencies-graph": "1.0.0-alpha.12",
    "codelyzer": "2.0.1",
    "compodoc": "0.0.41",
    "conventional-changelog-cli": "1.3.1",
    "jasmine-core": "2.5.2",
    "jasmine-spec-reporter": "3.2.0",
    "karma": "1.5.0",
    "karma-chrome-launcher": "2.0.0",
    "karma-cli": "1.0.1",
    "karma-coverage-istanbul-reporter": "1.0.0",
    "karma-jasmine": "1.1.0",
    "karma-jasmine-html-reporter": "0.2.2",
    "npm-check": "5.4.0",
    "protractor": "5.1.1",
    "rimraf": "2.6.1",
    "ts-node": "3.0.2",
    "tslint": "4.5.1",
    "typescript": "2.2.1"
  },
  "scripts": {
    "clean": "npm cache clean && rimraf coverage src/main/resources/static/*",
    "clean:dist": "rimraf src/main/resources/static/*",
    "preclean:install": "npm run clean",
    "clean:install": "npm upgrade",
    "preclean:start": "npm run clean",
    "clean:start": "npm start",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s -r 0",
    "docs": "compodoc -n Shardis -p src/main/typescript/tsconfig.app.json",
    "e2e": "ng e2e",
    "e2e:live": "ng e2e --elementExplorer",
    "lint": "ng lint",
    "pretest": "rimraf coverage && npm run lint",
    "test": "ng test -w false --code-coverage --progress false",
    "posttest": "npm run e2e",
    "test:live": "ng test --progress false",
    "build": "npm run build:dev",
    "prebuild:dev": "npm run clean:dist",
    "build:dev": "ng build -dev --extract-css=false",
    "prebuild:prod": "npm run clean:dist",
    "build:prod": "ng build -prod --aot --sourcemap=false --progress=false --extract-css",
    "server": "npm run server:dev",
    "server:dev": "ng serve --p 4200 -pc proxy.json -dev --open --progress=false --extract-css=false --sourcemap",
    "server:prod": "ng serve --p 4200 -pc proxy.json -prod --aot --open --progress=false --extract-css --sourcemap",
    "ng": "ng",
    "upgrade-interactive": "npm-check -uE --no-emoji",
    "start": "npm run server:dev"
  },
  "engines": {
    "node": ">= 7.7.4",
    "npm": ">= 4.4.1"
  }
}
