{
  "name": "shardis-angular",
  "version": "0.1.1-SNAPSHOT",
  "description": "shardis project",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/shardis/shardis.git"
  },
  "author": "Shardis",
  "license": "Apache-2",
  "bugs": {
    "url": "https://github.com/shardis/shardis/issues"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "4.1.1",
    "@angular/common": "4.1.1",
    "@angular/compiler": "4.1.1",
    "@angular/core": "4.1.1",
    "@angular/forms": "4.1.1",
    "@angular/http": "4.1.1",
    "@angular/platform-browser": "4.1.1",
    "@angular/platform-browser-dynamic": "4.1.1",
    "@angular/router": "4.1.1",
    "@ngrx/core": "1.2.0",
    "@ngrx/effects": "2.0.3",
    "@ngrx/router-store": "1.2.6",
    "@ngrx/store": "2.2.2",
    "@ngrx/store-devtools": "3.2.4",
    "@webcomponents/custom-elements": "1.0.0-alpha.3",
    "clarity-angular": "0.9.3",
    "clarity-icons": "0.9.3",
    "clarity-ui": "0.9.3",
    "core-js": "2.4.1",
    "mutationobserver-shim": "0.3.2",
    "ngrx-store-freeze": "0.1.9",
    "ngrx-store-localstorage": "0.1.7",
    "ngx-webstorage": "1.8.0",
    "reselect": "3.0.0",
    "rxjs": "5.3.1",
    "ts-helpers": "1.1.2",
    "web-animations-js": "2.2.5",
    "zone.js": "0.8.10"
  },
  "devDependencies": {
    "@angular/cli": "1.0.2",
    "@angular/compiler-cli": "4.1.1",
    "@compodoc/compodoc": "1.0.0-beta.8",
    "@types/jasmine": "2.5.47",
    "@types/node": "7.0.18",
    "codelyzer": "3.0.1",
    "conventional-changelog-cli": "1.3.1",
    "jasmine-core": "2.6.1",
    "jasmine-spec-reporter": "4.1.0",
    "karma": "1.7.0",
    "karma-chrome-launcher": "2.1.1",
    "karma-cli": "1.0.1",
    "karma-coverage-istanbul-reporter": "1.2.1",
    "karma-jasmine": "1.1.0",
    "karma-jasmine-html-reporter": "0.2.2",
    "protractor": "5.1.1",
    "rimraf": "2.6.1",
    "ts-node": "3.0.4",
    "tslint": "5.2.0",
    "typescript": "2.3.2",
    "yarn": "0.23.4"
  },
  "scripts": {
    "clean": "yarn cache clean && rimraf coverage src/main/resources/static/*",
    "clean:dist": "rimraf src/main/resources/static/*",
    "preclean:install": "yarn run clean",
    "clean:install": "yarn upgrade",
    "preclean:start": "yarn run clean",
    "clean:start": "yarn start",
    "changelog": "conventional-changelog -p angular -i ../CHANGELOG.md -s -r 0",
    "docs": "compodoc -n Shardis -p tsconfig.json",
    "e2e": "ng e2e",
    "e2e:live": "ng e2e --elementExplorer",
    "lint": "ng lint --type-check --fix",
    "pretest": "rimraf coverage && yarn run lint",
    "test": "ng test -w false --code-coverage --no-progress",
    "posttest": "yarn run e2e",
    "test:live": "ng test --no-progress",
    "build": "yarn run build:dev",
    "prebuild:dev": "yarn run clean:dist",
    "build:dev": "ng build -dev --sourcemaps -op src/main/resources/static",
    "prebuild:prod": "yarn run clean:dist",
    "build:prod": "ng build -prod --aot --no-progress --extract-css --sourcemaps -op src/main/resources/static",
    "server": "yarn run server:dev",
    "server:dev": "ng serve --p 4200 -pc proxy.json -dev --open --no-progress --sourcemaps",
    "server:prod": "ng serve --p 4200 -pc proxy.json -prod --aot --open --no-progress --extract-css --sourcemaps",
    "ng": "ng",
    "start": "yarn run server:dev"
  },
  "engines": {
    "node": ">= 7.10.0"
  }
}
